<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @log_post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @log_post.body %>
</p>

<p>
  <strong>User:</strong>
  <%= @log_post.user.first_name %> <%= @log_post.user.last_name %>
</p>

<p>
  <strong>City:</strong>
  <%= @log_post.city_id.name %>
</p>

<% if current_user.id == @log_post.user_id %>
	<%= link_to 'Edit', edit_log_post_path(@log_post) %>
<% end %>

<%= link_to 'Back', log_posts_path %>
